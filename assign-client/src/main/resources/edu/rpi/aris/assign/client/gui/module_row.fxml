<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" style="-fx-border-width: 0 0 1 0; -fx-border-color: grey;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="moduleImage" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" />
      <Separator visible="false" HBox.hgrow="ALWAYS" />
      <Button fx:id="moduleBtn" mnemonicParsing="false" onAction="#launchModule" text="Launch Module" />
   </children>
</HBox>
