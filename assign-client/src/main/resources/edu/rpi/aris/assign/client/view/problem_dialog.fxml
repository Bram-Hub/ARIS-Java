<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="box" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="name" promptText="Problem Name" />
      <ComboBox fx:id="module" maxWidth="1.7976931348623157E308" promptText="Problem Module" />
      <Button fx:id="btnEditor" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#openEditor" text="Open Problem Editor" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
